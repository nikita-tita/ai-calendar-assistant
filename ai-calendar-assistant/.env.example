# Application
APP_ENV=development
DEBUG=True
LOG_LEVEL=INFO
APP_BASE_URL=https://your-domain.com

# Telegram Bot
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_WEBAPP_URL=https://your-domain.com
TELEGRAM_WEBHOOK_SECRET=your_telegram_webhook_secret

# Radicale CalDAV Server (локальный многопользовательский календарь)
RADICALE_URL=http://radicale-calendar:5232
RADICALE_ADMIN_USER=your_radicale_admin_user
RADICALE_ADMIN_PASSWORD=your_radicale_admin_password

# Yandex GPT (работает из России без VPN)
YANDEX_GPT_API_KEY=your_yandex_gpt_api_key
YANDEX_GPT_FOLDER_ID=your_yandex_gpt_folder_id

# Server
HOST=0.0.0.0
PORT=8000

# Database (using SQLite by default)
# Use DATABASE_URL to override, e.g. DATABASE_URL=sqlite:///./data/app.db
DATABASE_URL=sqlite:///./data/app.db

# Admin Panel Credentials (Three-factor authentication)
ADMIN_PASSWORD_1=your_admin_password_1
ADMIN_PASSWORD_2=your_admin_password_2
ADMIN_PASSWORD_3=your_admin_password_3

# Legacy (kept for backward compatibility)
ADMIN_PRIMARY_PASSWORD=your_admin_primary_password
ADMIN_SECONDARY_PASSWORD=your_admin_secondary_password

# Timezone
DEFAULT_TIMEZONE=Europe/Moscow

# Rate Limiting
MAX_REQUESTS_PER_USER_PER_DAY=20
MAX_CONCURRENT_REQUESTS=100

# Redis Password
REDIS_PASSWORD=your_redis_password

# CORS Allowed Origins (comma-separated)
CORS_ORIGINS=https://yourdomain.example,https://webapp.telegram.org

# Radicale Service Account
RADICALE_BOT_USER=calendar_bot
RADICALE_BOT_PASSWORD=your_radicale_bot_password

# Property Search Bot Configuration
PROPERTY_BOT_ENABLED=true
PROPERTY_FEED_URL=your_property_feed_url
PROPERTY_DATABASE_URL=sqlite:///./data/property_bot.db